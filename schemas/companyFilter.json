{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "properties": {
        "name": {
            "type": "string",
            "default": "",
            "title": "The name Schema",
            "examples": [
                "Google"
            ]
        },
        "minEmployees": {
            "type": "integer",
            "default": 0,
            "title": "The minEmployees Schema",
            "examples": [
                1000
            ]
        },
        "maxEmployees": {
            "type": "integer",
            "title": "The maxEmployees Schema",
            "examples": [
                10000
            ]
        }
    },
    "additionalProperties": false,
    "examples": [{
        "name": "Google",
        "minEmployees": 1000,
        "maxEmployees": 10000
    }]
}